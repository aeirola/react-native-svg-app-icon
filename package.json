{
	"name": "react-native-svg-app-icon",
	"version": "0.6.1",
	"description": "App icon generator for React Native projects",
	"keywords": [
		"react-native",
		"svg",
		"app-icon"
	],
	"homepage": "https://github.com/aeirola/react-native-svg-app-icon",
	"bugs": {
		"url": "https://github.com/aeirola/react-native-svg-app-icon/issues"
	},
	"files": [
		"assets/",
		"lib/"
	],
	"bin": {
		"react-native-svg-app-icon": "lib/cli.js"
	},
	"main": "lib/index.js",
	"type": "commonjs",
	"scripts": {
		"prepare": "tsc --project tsconfig.build.json",
		"test": "concurrently 'npm:test:*'",
		"test:lint:style": "biome check",
		"test:lint:types": "tsc",
		"test:unit": "vitest run"
	},
	"author": "Axel Havukangas <axel@havukangas.fi>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/aeirola/react-native-svg-app-icon.git"
	},
	"dependencies": {
		"commander": "^14.0.3",
		"fs-extra": ">=3.0.0",
		"sharp": ">=0.23.0",
		"svg2vectordrawable": "2.9.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.13",
		"@tsconfig/node20": "^20.1.8",
		"@tsconfig/strictest": "^2.0.8",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^20.14.8",
		"@types/tmp": "^0.2.6",
		"concurrently": "^9.2.1",
		"glob": "^13.0.0",
		"pixelmatch": "^7.1.0",
		"tmp": "^0.2.5",
		"typescript": "^5.9.3",
		"vitest": "^4.0.9"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"devEngines": {
		"runtime": {
			"name": "node",
			"version": ">=20"
		},
		"packageManager": {
			"name": "npm",
			"version": ">=9"
		}
	}
}
